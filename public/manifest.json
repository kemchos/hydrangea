{
  "manifest_version": 2,
  "name": "Hydrangea",
  "short_name": "Hydrangea",
  "version": "1.0.1",
  "description": "Make it easy to access bookmarks via popup",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "Unfold Bookmarks"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}